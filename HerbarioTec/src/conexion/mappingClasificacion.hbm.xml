<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="herbariotec.Clasificacion" table="ClasificacionesTaxonomicas">
      <composite-id>
          <key-property name="familia" column="Familia"/>
          <key-property name="genero" column="Genero"/>
          <key-property name="especie" column="Especie"/>
      </composite-id>
      <property name="autor">
          <column name="Autor"></column>
      </property>
  </class>
</hibernate-mapping>
