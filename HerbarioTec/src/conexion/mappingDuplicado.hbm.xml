<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="herbariotec.Duplicado" table="Duplicados">
        <id column="IdDuplicado" name="identificacion" type="int">
            <generator class="assigned"></generator>            
        </id>
        <property name="acronimoMuestra">
            <column name="AcronimoMuestra"></column>
        </property>
        <property name="consecutivoMuestra">
            <column name="ConsecutivoMuestra"></column>
        </property>
        <property name="destino">
            <column name="Destino"></column>
        </property>
    </class>
</hibernate-mapping>
