<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="herbariotec.Imagen" table="Imagenes">
        <id name="nombre" column="Nombre">
            <generator class="assigned"></generator>
        </id>
        <property name="familia">
            <column name="Familia"></column>
        </property>
        <property name="genero">
            <column name="Genero"></column>
        </property>
        <property name="especie">
            <column name="Especie"></column>
        </property>
    </class>
</hibernate-mapping>
